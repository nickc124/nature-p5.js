let walk 
function setup() {
  createCanvas(400, 400);
  walk = new walker();
}

function draw() {
  background(220);
  walk.step();
  walk.show();
}

class walker{
  constructor(){
    this.pos = createVector(200,200);
    this.vel = p5.Vector.random2D();
  }
  step(){
    this.pos.add(this.vel);
    this.vel = p5.Vector.random2D();
  }
  show(){
    fill(255,100,0,150);
    circle(this.pos.x,this.pos.y,20);
  }
}
